<p id="notice"><%= notice %></p>

<h1>Start Date Options</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @start_date_options.each do |start_date_option| %>
      <tr>
        <td><%= link_to 'Show', start_date_option %></td>
        <td><%= link_to 'Edit', edit_start_date_option_path(start_date_option) %></td>
        <td><%= link_to 'Destroy', start_date_option, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Start Date Option', new_start_date_option_path %>
